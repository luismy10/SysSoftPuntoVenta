<AnchorPane id="AnchorPane" fx:id="window" stylesheets="@../styles.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controller.FxProveedorProcesoController">
