<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FxReporteInventarioController">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <HBox style="-fx-padding: 0.8333333333333334em;">
               <children>
                  <Label styleClass="labelOpenSansRegular18" text="Reporte de inventario" />
               </children>
            </HBox>
            <HBox style="-fx-padding: 0.8333333333333334em;" VBox.vgrow="ALWAYS">
               <children>
                  <TableView fx:id="tvList" minHeight="-Infinity" minWidth="-Infinity" HBox.hgrow="ALWAYS">
                     <columns>
                        <TableColumn prefWidth="75.0" text="N°" />
                        <TableColumn prefWidth="280.0" text="Artículo" />
                        <TableColumn prefWidth="120.0" text="Costo" />
                        <TableColumn prefWidth="120.0" text="Precio Venta" />
                        <TableColumn prefWidth="120.0" text="Existencias" />
                        <TableColumn prefWidth="140.0" text="Medidad" />
                        <TableColumn prefWidth="140.0" text="Inv. Mínimo" />
                        <TableColumn prefWidth="140.0" text="Inv. Máximo" />
                     </columns>
                  </TableView>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
